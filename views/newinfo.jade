extends layout

block content
    #page
        include common/header
        #content.clearfix
            include common/nav_top
            #banner
                img(src="/statics/images/banner2.jpg")
            include common/nav_left
            #main
                .productInfo.news
                    h2.tc #{newinfo.title}
                    - var new_cont = newinfo.content
                    .new_cont!= new_cont


        include common/footer
    input(type='hidden' id="currLg" value="#{lg}")
    script(src="/statics/API.js")
    script(src="/statics/js/index.js")